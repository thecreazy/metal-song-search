            },
            {
                "startTimeMs": "0",
                "words": "♪",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Róka hasa telelő, felhőn gurul az idő",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Halkan rezeg a mező alján valami erő",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Este leesik a hó, csend van, kiköt a hajó",
