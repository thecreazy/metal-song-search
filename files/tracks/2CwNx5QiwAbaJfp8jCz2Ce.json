            },
            {
                "startTimeMs": "0",
                "words": "Alvó libalegelő, zúgó libalegelő",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Kádam vizén a hajó, bentről szól egy rádió",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "éjjel anya hallható, nappal apa hogyha jó az adó",
