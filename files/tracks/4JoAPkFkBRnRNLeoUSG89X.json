        "lines": [
            {
                "startTimeMs": "0",
                "words": "Far to the east ruled the Samurai, mystic warrior class,",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Honorable brave, bark armoured man, donning hideous mask.",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "700 years of reigning Japan, the masters of attack,",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Dominators of the isle of dragons, fearsom military caste.",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "In battle no prisoners for the enemy, noble self-sacrifice,",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Crooked crowns sentenced to die, pay the bloody price.",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Sacred rites held close by the fighters, no place for disgrace,",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Taught to stand with dignity while staring in death's face.",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Honor even in death, soldiers glorified,",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "The dagger plunges deep, slashes side to side.",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Honor even in death, digrace is defied,",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "The end they never fear, give their life before their pride.",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Honor even in death, soldiers glorified,",
                "syllables": []
            },
            {
